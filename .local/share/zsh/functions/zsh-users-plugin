name=$1
if [[ -n "$HOMEBREW_PREFIX" ]] && [[ -f "$HOMEBREW_PREFIX/share/$name/$name.zsh" ]]; then
  source "$HOMEBREW_PREFIX/share/$name/$name.zsh"
  return $?
elif [[ -f "/usr/share/$name/$name.zsh" ]]; then
  source "/usr/share/$name/$name.zsh"
  return $?
elif type "zplug" >/dev/null 2>&1; then
  zplug "zsh-users/$name"
  return $?
fi

return 1
